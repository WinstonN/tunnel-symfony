{
    "name": "codeium/tunnel-symfony",
    "description": "AWS SSM Port Forwarding Tunnel Manager",
    "type": "project",
    "license": "MIT",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": ">=8.1",
        "symfony/console": "^6.3",
        "symfony/yaml": "^6.3",
        "aws/aws-sdk-php": "^3.283",
        "symfony/process": "^6.3",
        "symfony/dependency-injection": "^6.3",
        "symfony/config": "^6.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.4",
        "symfony/phpunit-bridge": "^6.3"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    }
}
